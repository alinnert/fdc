{
  "name": "fdth",
  "version": "1.0.0",
  "description": "Fonto Development Tools Helpers",
  "main": "dist/fdth",
  "bin": {
    "fdth": "./dist/fdth"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:ui": "vite",
    "start:server": "npm run build:server && cross-env NODE_ENV=development node ./dist/fdth",
    "build": "npm-run-all build:*",
    "build:ui": "vite build",
    "build:server": "esbuild server/main.ts --bundle --platform=node --target=node10.4 --external:./node_modules/* --outfile=./dist/fdth"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "fuzzysort": "^1.1.4",
    "mri": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.21",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "tailwindcss": "^3.0.22",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.1"
  }
}
