{
  "name": "@alinnert/fdc",
  "version": "1.0.0",
  "description": "Fonto Dev Companion",
  "exports": "./dist/fdc.js",
  "type": "module",
  "bin": {
    "fdc": "./dist/fdc.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:ui": "vite",
    "start:server": "npm run build:server && cross-env NODE_ENV=development node ./dist/fdc.js",
    "build": "npm-run-all build:*",
    "build:ui": "vite build",
    "build:server": "esbuild server/main.ts --bundle --platform=node --target=node14.0 --format=esm --external:./node_modules/* --outfile=./dist/fdc.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.6",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fuzzysort": "^2.0.1",
    "globby": "^13.1.2",
    "mri": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "recoil": "^0.7.6"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.10",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
